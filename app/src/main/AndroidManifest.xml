<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rquiz">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_bg"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo_circle"
        android:supportsRtl="true"
        android:theme="@style/Theme.RQuiz">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".AboutActivity"
            android:exported="true"
            android:label="About"
            android:theme="@style/Theme.RQuiz.DarkActionBar">
        </activity>

        <activity android:name=".Subject"
            android:exported="true"
            android:label="Subject">
        </activity>

        <activity android:name=".Settings"
            android:exported="true"
            android:label="Settings"
            android:theme="@style/Theme.RQuiz.DarkActionBar">
        </activity>

        <activity android:name=".ForgetPassword"
            android:exported="true"
            android:label="Forget Password">
        </activity>

        <activity android:name=".ResetPassword"
            android:exported="true"
            android:label="Reset Password">
        </activity>

        <activity android:name=".QuizQuestions"
            android:exported="true"
            android:label="RQuiz"
            android:theme="@style/Theme.RQuiz.DarkActionBar">
        </activity>

        <activity android:name=".Register"
            android:exported="true"
            android:label="Register">
        </activity>

        <activity android:name=".Score"
            android:exported="true"
            android:label="Score"
            android:theme="@style/Theme.RQuiz.DarkActionBar">
        </activity>

        <activity android:name=".History"
            android:exported="true"
            android:label="History"
            android:theme="@style/Theme.RQuiz.DarkActionBar">
        </activity>

    </application>


</manifest>